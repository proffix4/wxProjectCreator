{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Запуск и отладка myappd.exe (Debug)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/myappd.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "externalConsole": "true",
            "MIMode": "gdb",
            "miDebuggerPath": "D:/Development/RedPanda-CPP/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Разрешить печать символов в gdb",
                    "text": "-gdb-set new-console on",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Скомпилировать wxWidgets-приложение (Debug)",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
